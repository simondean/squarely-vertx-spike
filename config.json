{
  "server": {
    "redis": {
      "address": "io.squarely.vertxspike.redis",
      "host": "localhost",
      "port": 6379
    }
  },
  "jenkins": {
    "servers": [
      {
        "host": "builds.apache.org",
        "port": 443,
        "ssl": true,
        "jobLimit": 1000
      },
      {
        "host": "jenkins.nodejs.org",
        "port": 80,
        "ssl": false,
        "jobLimit": 1000
      }
    ],
    "metricNamePrefix": "ci.jenkins"
  },
  "sonarQube": {
    "servers": [
      {
        "host": "sonar.spring.io",
        "port": 443,
        "ssl": true,
        "projectLimit": 1000
      },
      {
        "host": "dev.eclipse.org",
        "port": 443,
        "path": "/sonar",
        "ssl": true,
        "projectLimit": 1000
      }
    ],
    "metricNamePrefix": "ci.sonarqube"
  }
}
